import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=a/10;
        int c=a%10;
        int sum=b+c;
        System.out.println("Bird said:"+a);
        System.out.println("Alice must go in path-"+sum);
    }
}
