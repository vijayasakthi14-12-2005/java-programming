import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
        int d=sc.nextInt();
        int e=sc.nextInt();
        System.out.println("Sum of the 5 number is : " + (a+b+c+d+e));
        System.out.println("Average of the 5 number is : " + (float)(a+b+c+d+e)/5);
    
    
    
