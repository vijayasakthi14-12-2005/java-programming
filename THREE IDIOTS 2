import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
        int x1=sc.nextInt();
        int x2=sc.nextInt();
        int y1=sc.nextInt();
        int y2=sc.nextInt();
        double x=(x1+y1)/2.0;
        double y=(x2+y2)/2.0;
        System.out.println("Binoy's house is located at " + "(" + x + "," + y + ")");
    }
}
